<?php
// Fungsi untuk mendeteksi perangkat mobile atau desktop
function isMobileDevice() {
    return preg_match('/android|webos|iphone|ipad|ipod|blackberry|iemobile|opera mini/i', strtolower($_SERVER['HTTP_USER_AGENT']));
}
