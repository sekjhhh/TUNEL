        <div id="billboard" class="billboard-top"
        style="background-color:#FAFAFA;width: 970px; height: 250px; z-index: 13;position: relative; top: 0; transition: opacity 1s, top 0.3s; opacity: 1;">
            <div class="parallax1 parallaxB" id="megabillboard-desktop"
                style="width: 100%;height: auto;position: relative;margin-bottom:5px !important;margin-top:0 !important;max-width:970px;left:0;right:0;margin:auto;">
                <div class="parallax_abs" style="height:auto;">
                    <div class="parallax_fix"
                        style="transition: 0.3s ease-out; width: auto; height: auto;top: 0; position: relative; background: #f8f8f8; width: 100%; max-width: fit-content; left: 0; right: 0; margin: auto;">
                        <!-- /4905536/detik_desktop/news/billboard -->
