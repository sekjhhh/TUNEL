    <script>
        let billboard = document.getElementById('billboard');
        let paraAbs = billboard.querySelector('.parallax_abs');
        let paraFix = billboard.querySelector('.parallax_fix');
        let timerStarted = false;
        let isSticky = false;
        let stickyTriggered = false;
        let BBTopFlag = false;

        function isBBInViewport() {
            const rect = billboard.getBoundingClientRect();
            return (
                rect.top >= 0 &&
                rect.left >= 0 &&
                rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
                rect.right <= (window.innerWidth || document.documentElement.clientWidth)
            );
        }

        function stickyBB() {
            if (isBBInViewport() || BBTopFlag) {
                BBTopFlag = true;
                if (!stickyTriggered && !isSticky && billboard.getBoundingClientRect().top <= 0) {
                    Object.assign(billboard.style, {
                        position: 'sticky',
                        top: '46px',
                    });
                    Object.assign(paraAbs.style, {
                        position: 'relative'
                    })
                    Object.assign(paraFix.style, {
                        top: '46px',
                        position: 'fixed',
                        padding: '16px'
                    })

                    isSticky = true;
                    stickyTriggered = true;

                    if (!timerStarted) {
                        timerStarted = true;
                        setTimeout(function () {
                            billboard.style.opacity = '0';

                            setTimeout(function () {
                                Object.assign(billboard.style, {
                                    position: 'relative',
                                    top: '0',
                                    opacity: '1',
                                    margin: 'inherit'
                                });
                                Object.assign(paraAbs.style, {
                                    position: 'absolute'
                                })
                                Object.assign(paraFix.style, {
                                    top: '0',
                                    position: 'relative',
                                    padding: 'unset'
                                })

                                isSticky = false;
                                timerStarted = false;
                            }, 1000);
                        }, 4000);
                    }
                } else if (stickyTriggered && isSticky) {
                    const isDetail = $(".sticky-share").length > 0;
                    const navbarSticky = $(".navbar-sticky").hasClass("is-sticky");
                    const stickyShareSticked = $(".sticky-share").hasClass("sticky-share--sticked");

                    function updateTopPosition(topValue) {
                        Object.assign(paraFix.style, { top: `${topValue}px` });
                    }

                    if (window.scrollY === 0) {
                        updateTopPosition(46);
                    } else if (!navbarSticky) {
                        updateTopPosition(isDetail ? 0 : 46);
                    } else if ((isDetail && stickyShareSticked)) {
                        updateTopPosition(88);
                    } else if (navbarSticky) {
                        updateTopPosition(isDetail ? 46 : 88);
                    }
                }
            }
        
        }
        
        window.addEventListener('scroll', function () {
            stickyBB();
        })
    </script>
